@{
    ViewBag.Title = "IndicadorEntregas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid" style="height:93%;">

    <div class="panel">
        <div class="container-fluid">
            <div class="topbar-left col-xs-6">


                <h3>Indicador de Entregas a tiempo</h3>
            </div>
        </div>
    </div>
    <div class="panel">
        <div class="container-fluid">

            <a id="reporteRPT" style="display:none"></a>
            <div class="panel-footer text-right">
                <div class="row">
                    <input class="form-control input-sm" id="Input" placeholder="Ingrese Año" />
                    <button type="button" id="btnReporte" class="btn btn-primary pull-centers plu" onclick="GenerarIndicadorEntregas('pdf')">Generar Reporte</button>
                    <!--<button type="button" id="btnReporte" class="btn btn-success pull-centers plu" style="background-color:green" onclick="GenerarIndicadorEntregas('excel')">Generar Excel</button>-->

                </div>
            </div>

        </div>
    </div>

</div>

<script src="~/Scripts/ReporteLogistica/IndicadorEntregas.js"></script>
<script src="~/Scripts/General.js"></script>
<script src="~/js/pdf/jspdf.min.js"></script>
