@{
    ViewBag.Title = "CompraAnual";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid" style="height:93%;">

    <div class="panel">
        <div class="container-fluid">
            <div class="topbar-left col-xs-6">
                <h3>Proveedor Unico</h3>
            </div>
        </div>
    </div>
    <div class="panel">
        <div class="container-fluid">

            <a id="reporteRPT" style="display:none"></a>
            <div class="panel-footer text-left">

                <div class="row">
                    <div class='form-group-sm'>
                        <label for="FechaInicio" class="control-label col-sm-2 left-align">Fecha Inicio</label>
                        <div class="col-sm-4">
                            <input id="FechaInicio" type="date" class="form-control input-sm" tabindex="0">
                        </div>
                    </div>
                    <div class='form-group-sm'>
                        <label for="cboTipoPersona" class="control-label col-sm-2 rigth-align">Fecha Fin</label>
                        <div class="col-sm-4">
                            <input id="FechaFin" type="date" class="form-control input-sm" tabindex="0">
                        </div>
                    </div>
                </div>
                <div class="row">
                    <button type="button" id="btnReporte" class="btn btn-primary pull-centers plu" onclick="GenerarReporteProveedorUnico()">Generar Reporte</button>
                </div>
            </div>

        </div>
    </div>

</div>

<script src="~/Scripts/ReporteLogistica/ProveedorUnico.js"></script>
<script src="~/Scripts/General.js"></script>
<script src="~/js/pdf/jspdf.min.js"></script>
<script src="~/js/pdf/faker.min.js"></script>
<script src="~/js/pdf/jspdf.plugin.autotable.src.js"></script>


