@{

    @using System.Reflection
    @using System.Diagnostics
    @using System.Text.RegularExpressions

    var asm = typeof(ConcyssaWeb.Controllers.HomeController).Assembly;

    // Intentar leer InformationalVersion (donde suele estar 1.0.0+hash)
    var infoVersion = asm.GetCustomAttribute<AssemblyInformationalVersionAttribute>()?.InformationalVersion
                      ?? FileVersionInfo.GetVersionInfo(asm.Location).ProductVersion
                      ?? asm.GetName().Version?.ToString();

    // Extraer solo la parte semver: X.Y.Z o X.Y.Z-pre
    var match = Regex.Match(infoVersion ?? "", @"(?<semver>\d+\.\d+\.\d+(-[0-9A-Za-z\.-]+)?)");
    var version = match.Success ? match.Groups["semver"].Value : "0.0.0";
}


